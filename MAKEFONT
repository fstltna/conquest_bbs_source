# largefont.txf

gentexfont -fn '-*-helvetica-bold-r-*-*-*-240-*-*-*-*-*-*' -file large.txf -glist '`"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ01234567890 \!@#$$%^&*()-=+/,.<>;:~{}[]' -w 256 -h 256 -gap 3

# msg

gentexfont -fn '-b&h-luxi sans-medium-r-normal--37-360-75-75-p-204-iso8859-15' -file msg.txf -glist '`"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ01234567890 \!@#$$%^&*()-=+/,.<>;:~{}[]?_~|'''\' -w 256 -h 256 -gap 3

# fixed
gentexfont -fn '-Adobe-Courier-Bold-R-Normal--20-140-100-100-M-110-ISO8859-9' -file fixed.txf -glist '`"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ01234567890 \!@#$$%^&*()-=+/,.<>;:~{}[]?_~|'''\' -w 256 -h 256 -gap 3

#tinyfixed
gentexfont -fn '-dt-interface user-bold-r-normal-m sans-*-*-100-100-m-*-*-*' -file tinyfixed.txf -glist '`"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ01234567890 \!@#$$%^&*()-=+/,.<>;:~{}[]?_~|'''\' -w 256 -h 256 -gap 3
